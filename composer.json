{
    "name": "datagutten/imagehost",
    "description": "A PHP class structure to upload images to different hosts",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/datagutten/Requests.git"
    }
  ],
    "require": {
      "ext-json": "*",
      "ext-curl": "*",
      "symfony/filesystem": "^4.3",
      "rmccue/requests": "^1.7",
      "cweagans/composer-patches": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "datagutten\\image_host\\": "sites"
        }
    },
  "extra": {
    "patches": {
      "rmccue/requests": {
        "Support multipart/form-data": "https://github.com/datagutten/Requests/commit/97cd8b6422cbf2f5bd3040b4d70f1629437b521c.patch"
      }
    }
  }
}
