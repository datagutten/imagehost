{
    "name": "datagutten/imagehost",
    "description": "A PHP class structure to upload images to different hosts",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9|^8|^7.5"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
    "require": {
      "php": ">=7.1",
      "ext-json": "*",
      "ext-curl": "*",
      "symfony/filesystem": "^4.3|^5",
      "rmccue/requests": "1.8.1",
      "cweagans/composer-patches": "~1.0"
    },
  "config": {
    "preferred-install": "source",
    "sort-packages": true
  },
  "autoload": {
        "psr-4": {
            "datagutten\\image_host\\": "sites"
        }
    },
    "extra": {
        "patches": {
            "rmccue/requests": {
                "Support multipart/form-data": "https://github.com/datagutten/Requests/commit/3c88c2fef11abd875dc7b4f376312901d63c79d1.patch"
            }
        }
    }
}