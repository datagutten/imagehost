{
    "name": "datagutten/imagehost",
    "description": "A PHP class structure to upload images to different hosts",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/filesystem": "^5.4"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
    "require": {
      "datagutten/tools": "^1.10",
      "ext-dom": "*",
      "ext-json": "*",
      "ext-curl": "*",
      "rmccue/requests": "^2.0",
      "php": "^8.0"
    },
  "config": {
    "preferred-install": "source",
    "sort-packages": true
  },
  "autoload": {
        "psr-4": {
            "datagutten\\image_host\\": "src",
            "datagutten\\image_host\\sites\\": "sites",
            "datagutten\\image_host\\tests\\": "tests"
        }
    }
}
