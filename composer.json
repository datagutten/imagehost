{
    "name": "datagutten/imagehost",
    "description": "A PHP class structure to upload images to different hosts",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9|^8|^7.5"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
    "require": {
      "php": ">=7.1",
      "ext-json": "*",
      "ext-curl": "*",
      "symfony/filesystem": "^4.3|^5",
      "rmccue/requests": "dev-multipart-form-data-v1.8.1"
    },
  "config": {
    "preferred-install": "source",
    "sort-packages": true
  },
  "autoload": {
        "psr-4": {
            "datagutten\\image_host\\": "sites"
        }
    },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/datagutten/Requests"
    }
  ]
}
